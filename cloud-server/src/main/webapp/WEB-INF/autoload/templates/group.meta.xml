<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE c:meta SYSTEM "http://clyde.ettrema.com/dtd/core.dtd"><c:meta xmlns:c="http://clyde.ettrema.com/ns/core">	<c:template instanceType="group" template="normal">		<c:fields>			<c:html name="body" rows="30" cols="80" required="false" disAllowTemplating="false" toolbar="Default" />		</c:fields>		<c:components>			<c:subpage name="index.html" template="normal">				<c:attributes>					<c:value name="title">$targetPage.foundParent.name</c:value>					<c:value name="body">						<div style="padding: 15px">							<h1>Group Members</h1>							#foreach($user in $targetPage.parent.members)							<li>$user.link</li>							#end						</div>					</c:value>				</c:attributes>			</c:subpage>		</c:components>	</c:template></c:meta>